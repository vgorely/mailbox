{
  "name": "Mailbox",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "~7.1",
    "slim/slim": "~3.8.1",
    "doctrine/dbal": "~2.6.2",
    "doctrine/migrations": "~1.5.0",
    "justinrainbow/json-schema": "~5.2.0",
    "monolog/monolog": "~1.23.0",
    "symfony/console": "~3.2.7",
    "zendframework/zenddiagnostics": "~1.0.9"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.4.3",
    "phpunit/php-code-coverage" : "~5.2.2"
  },
  "autoload": {
    "psr-4": {
      "Mailbox\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Mailbox\\": "tests/"
    }
  }
}
